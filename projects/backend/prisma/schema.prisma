generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Vehicle {
    id      String @id @default(uuid())
    plate   String
    chassi  String
    renavam String
    model   String
    brand   String
    year    Int

    @@map("vehicles")
}
